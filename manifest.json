{
  "name": "MyNextPath",
  "version": "1.0",
  "manifest_version": 3,
 
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["script.js"]
    }
  ],
  "permissions": ["tabs"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png",
      "48": "images/48.png",
      "128": "images/128.png"
    }
  }
}
